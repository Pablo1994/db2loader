                        <ComboBox id="_comboTablas" fx:id="_comboTablas" layoutX="141.0" layoutY="27.0" onAction="#cambiarAtributos" prefWidth="150.0" />
                    </children>
                </Pane>
                <Pane layoutX="18.0" layoutY="321.0" prefHeight="71.0" prefWidth="583.0">

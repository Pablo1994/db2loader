                  <Button id="_buttonLimpiaSelect" fx:id="_buttonLimpiaSelect" layoutX="560.0" layoutY="128.0" mnemonicParsing="false" onAction="#limpiarSelecionados" prefHeight="25.0" prefWidth="68.0" text="Limpiar" />

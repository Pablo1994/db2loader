              <Button fx:id="button" alignment="BOTTOM_RIGHT" blendMode="DIFFERENCE" contentDisplay="BOTTOM" layoutX="549.0" layoutY="428.0" onAction="#cargarDatos" text="Cargar Datos" />
            </children>
        </Pane>
    </children>
</AnchorPane>

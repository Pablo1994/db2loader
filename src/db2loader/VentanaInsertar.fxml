                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font></Label>
                        <TextField id="_textRuta" fx:id="_textRuta" editable="false" layoutX="169.0" layoutY="27.0" prefHeight="25.0" prefWidth="156.0" promptText="Ruta del Archivo  .txt" />
